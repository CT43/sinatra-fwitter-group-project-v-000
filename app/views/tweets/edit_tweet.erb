
<h1>Edit Tweet</h1>
<%= @tweet.content %>
<form action="/tweets/<%=@tweet.id%>" method="POST">
  <input type="hidden" name="_method" value="PATCH">

    <h4>Edit Tweet:</h4>
    content: <input type="text" id="content" name="content" value="<%=@tweet.content%>">

    <input type="submit" value="submit">
  </form>
